<%= render "layouts/header" %>
<div id="main-container">
	<div id="main" class="main main-content">
		<%= form_for @story do |f| %>
		<%= f.label :title %>:
		<%= f.text_field :title %><br />

		<%= f.label :content %>:
		<%= f.text_area :content %><br />

		<%= f.submit %>
		<% end %>
	</div>
</div>